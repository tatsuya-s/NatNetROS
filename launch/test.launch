<launch>

  <arg name="local_address" default="192.168.1.2"/>
  <arg name="server_address" default="192.168.1.1"/>

  <node pkg="natnet_ros" type="natnet_node" name="natnet_node" output="screen">
    <rosparam subst_value="true">
      local_address: $(arg local_address)
      server_address: $(arg server_address)
      z_up: true
    </rosparam>
  </node>

</launch>